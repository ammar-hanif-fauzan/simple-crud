        "/api/v1/register": {
            "post": {
        "summary": "Register new user",
        "description": "Mendaftarkan user baru",
        "tags": ["Authentication"],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                "type": "object",
                                "properties": {
                                    "name": {
                                        "type": "string",
                    "example": "John Doe"
                                    },
                                    "email": {
                                        "type": "string",
                    "example": "john@example.com"
                                    },
                                    "password": {
                                        "type": "string",
                    "example": "password123"
                                    },
                                    "password_confirmation": {
                                        "type": "string",
                    "example": "password123"
