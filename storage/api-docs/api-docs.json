    "/api/v1/people": {
      "get": {
        "summary": "Get all people",
        "description": "Mengambil daftar semua orang dengan relasi idCard, hobbies, dan phoneNumber",
        "tags": ["People"],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "current_page": {
                      "type": "integer"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "integer"
                          },
                          "name": {
                            "type": "string"
                          },
                          "created_at": {
                            "type": "string",
                            "format": "datetime"
                          },
                          "updated_at": {
                            "type": "string",
                            "format": "datetime"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create new person",
        "description": "Membuat orang baru dengan idCard dan hobby",
        "tags": ["People"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "John Doe"
                  },
                  "id_number": {
                    "type": "string",
                    "example": "1234567890"
                  },
                  "hobby_id": {
                    "type": "array",
                    "items": {
                      "type": "integer"
                    },
                    "example": [1, 2]
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Person created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Person created successfully"
                    },
                    "data": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
